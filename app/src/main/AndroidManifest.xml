<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.emode">

    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_appicon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_appicon_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".HomeFragment"></activity>
        <activity
            android:name=".Sobre"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".SignUp"
            android:parentActivityName=".SignIn" />
        <activity
            android:name=".PacienteSelecionar"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".SessaoAdd"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".SessaoActivity"
            android:label="@string/title_activity_sessao"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".PacienteAdd"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".SignIn">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>